<?xml version="1.0"?>

<project default="defaultTest">


	<tstamp>
			<format property="now2" pattern="MMdhm" />
	</tstamp>
	
	
	<target name="defaultTest" depends="homepageAlive,fullTest" />
	
	<condition property="isProd">
			<equals arg1="${host}" arg2="${prodHost}" casesensitive="false" trim="true" />
	</condition>
	
	<condition property="isSingleSignIn">
			<equals arg1="${enableSingleSignIn}" arg2="1" casesensitive="false" trim="true" />
	</condition>

	<condition property="isAllowMixedCart">
			<equals arg1="${allowMixedCart}" arg2="1" casesensitive="false" trim="true" />
	</condition>
	
	<condition property="isAPPCustomizationPage">
			<equals arg1="${aPPCustomizationPage}" arg2="1" casesensitive="false" trim="true" />
	</condition>
	
	<condition property="isPCMOnly">
			<equals arg1="${store}" arg2="pcm" casesensitive="false" trim="true" />
	</condition>
	
	
	<!-- set test case if to execute -->
	<condition property="isSearch">
			<contains substring="isSearch" string="${testcase.selected}"/>
	</condition>
	<condition property="isHomeMyAccount">
			<contains substring="isHomeMyAccount" string="${testcase.selected}"/>
	</condition>
	<condition property="isShoppingCart">
			<contains substring="isShoppingCart" string="${testcase.selected}"/>
	</condition>
	<condition property="isVerifyOrderCheckout">
			<contains substring="isVerifyOrderCheckout" string="${testcase.selected}"/>
	</condition>
	<condition property="isLogin">
			<contains substring="isLogin" string="${testcase.selected}"/>
	</condition>
	<condition property="isUserInfoFieldValidation">
			<contains substring="isUserInfoFieldValidation" string="${testcase.selected}"/>
	</condition>
	<condition property="isVerifyLinks">
			<contains substring="isVerifyLinks" string="${testcase.selected}"/>
	</condition>
	<condition property="isPriceRebatesVerification">
			<contains substring="isPriceRebatesVerification" string="${testcase.selected}"/>
	</condition>
	
	
	
	<!-- set type of test to execute -->
	<condition property="isBasic">
			<contains substring="Basic" string="${testtype.selected}"/>
	</condition>
	
	<condition property="isFull">
			<contains substring="Full" string="${testtype.selected}"/>
	</condition>
	
	
	<target name="fullTest">
		<antcall target="verifyOrderCheckout"/>
		<antcall target="shoppingCart"/>
		<antcall target="isSearch"/>
		<antcall target="isHomeMyAccount"/>
